{
"name" :"Messenger Link",
"version" :"1.0",
"description" :"Le truc du tutoriel",
"content_security_policy": "script-src 'self' 'unsafe-eval' https://www.gstatic.com https://*.firebaseio.com; object-src 'self';",
"permissions": [
  "activeTab",
  "declarativeContent",
  "storage"
],
"background": {
    "page": "background.html",
    "persistent": false
  },
"page_action":
	{
    "default_popup": "index.html",
    "default_icon": {
       "16": "src/assets/images/get_started16.png",
       "32": "src/assets/images/get_started32.png",
       "48": "src/assets/images/get_started48.png",
       "128": "src/assets/images/get_started128.png"
     }
	},
"icons": {
   "16": "src/assets/images/get_started16.png",
   "32": "src/assets/images/get_started32.png",
   "48": "src/assets/images/get_started48.png",
   "128": "src/assets/images/get_started128.png"
 },
"manifest_version": 2
}
